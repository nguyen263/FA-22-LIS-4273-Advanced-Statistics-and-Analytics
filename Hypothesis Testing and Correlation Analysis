Question 1
# a = given mean
a <- 70
# given standard deviation
s <- 3.5
# sample size
n <- 49
# mean of the samples
xbar <- 69.1
# calculate z score
z <- (xbar-a)/(s/sqrt(n))
z
# finding P-values with the pnorm function
pnorm(z)
#find critical value
qt(p=.05, df=48, lower.tail=TRUE)

# a = given mean
a <- 70
